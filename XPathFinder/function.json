{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httptrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post",
        "options"
      ],
      "route": "XPathFinder/{*segment}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/XPathFinder/index.js"
}